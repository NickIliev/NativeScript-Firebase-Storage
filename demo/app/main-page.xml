<Page xmlns="http://schemas.nativescript.org/tns.xsd" navigatingTo="navigatingTo">

    <StackLayout class="m-20">
        <Button text="Nav to sub-page" tap="nav" backgroundColor="blue" height="80"/>
    

        <Button text="Upload file" tap="{{ uploadFile }}" class="btn btn-primary btn-active"/>
        <Button text="Download File" tap="{{ downloadFile }}" class="btn btn-primary btn-active"/>
        <Button text="Get Downloadn URL" tap="{{ getDownloadUrl }}" class="btn btn-primary btn-active"/>
        <Button text="Delete File" tap="{{ deleteFile }}" class="btn btn-primary btn-active"/>
        <GridLayout rows="*" columns="*">
            <Label text="{{ message }}" class="text-center" textWrap="true" />
        </GridLayout>
    </StackLayout>

</Page>
